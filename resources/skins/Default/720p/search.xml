<window type="window">
    <defaultcontrol>1210</defaultcontrol>
    <coordinates>
        <system>1</system>
        <posx>0</posx>
        <posy>0</posy>
    </coordinates>
    <allowoverlay>no</allowoverlay>
    <controls>
        <control type="image">
            <description>background</description>
            <posx>0</posx>
            <posy>0</posy>
            <width>1280</width>
            <height>720</height>
            <texture>bg.png</texture>
        </control>
        <control type="image">
            <description>search-title</description>
            <posx>20</posx>
            <posy>20</posy>
            <width>160</width>
            <height>80</height>
            <texture>search.png</texture>
            <aspectratio>stretch</aspectratio>
        </control>
        <control type="image">
            <description>search-input</description>
            <posx>170</posx>
            <posy>110</posy>
            <width>400</width>
            <height>40</height>
            <texture>search-input.png</texture>
            <aspectratio>stretch</aspectratio>
        </control>
        <control type="group" id="1200">
            <control type="button" id="1201">
                <description>key-Chinese</description>
                <posx>170</posx>
                <posy>165</posy>
                <width>50</width>
                <height>50</height>
                <font>font8</font>
                <align>center</align>
                <texturefocus>search-input-small-focus.png</texturefocus>
                <texturenofocus>search-input-small.png</texturenofocus>
                <aspectratio>stretch</aspectratio>
                <ondown>1210</ondown>
                <onleft>1205</onleft>
                <onright>1202</onright>
            </control>
            <control type="button" id="1202">
                <description>key-space</description>
                <posx>240</posx>
                <posy>165</posy>
                <width>50</width>
                <height>50</height>
                <font>font8</font>
                <align>center</align>
                <texturefocus>search-input-small-focus.png</texturefocus>
                <texturenofocus>search-input-small.png</texturenofocus>
                <aspectratio>stretch</aspectratio>
                <ondown>1210</ondown>
                <onleft>1201</onleft>
                <onright>1203</onright>
            </control>
            <control type="button" id="1203">
                <description>key-clear</description>
                <posx>310</posx>
                <posy>165</posy>
                <width>50</width>
                <height>50</height>
                <font>font8</font>
                <align>center</align>
                <texturefocus>search-input-small-focus.png</texturefocus>
                <texturenofocus>search-input-small.png</texturenofocus>
                <aspectratio>stretch</aspectratio>
                <ondown>1210</ondown>
                <onleft>1202</onleft>
                <onright>1204</onright>
            </control>
            <control type="button" id="1204">
                <description>key-backspace</description>
                <posx>380</posx>
                <posy>165</posy>
                <width>50</width>
                <height>50</height>
                <font>font8</font>
                <align>center</align>
                <texturefocus>search-input-small-focus.png</texturefocus>
                <texturenofocus>search-input-small.png</texturenofocus>
                <aspectratio>stretch</aspectratio>
                <ondown>1210</ondown>
                <onleft>1203</onleft>
                <onright>1205</onright>
            </control>
            <control type="button" id="1205">
                <description>key-search</description>
                <posx>450</posx>
                <posy>165</posy>
                <width>120</width>
                <height>50</height>
                <font>font8</font>
                <align>center</align>
                <texturefocus>search-input-meduim-focus.png</texturefocus>
                <texturenofocus>search-input-meduim.png</texturenofocus>
                <aspectratio>stretch</aspectratio>
                <ondown>1210</ondown>
                <onleft>1204</onleft>
                <onright>1201</onright>
            </control>
        </control>
        <control type="panel" id="1210">
            <description>key-input</description>
            <posx>170</posx>
            <posy>235</posy>
            <width>420</width>
            <height>420</height>
            <viewtype>list</viewtype>
            <onleft>-</onleft>
            <onright>1220</onright>
            <onup>1200</onup>
            <ondown>-</ondown>
            <itemlayout width="70" height="70">
                <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>50</width>
                    <height>50</height>        
                    <texture>search-input-small.png</texture>
                    <aspectratio>stretch</aspectratio>
                </control>    
                <control type="label">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>50</width>
                    <height>50</height>   
                    <font>font8</font>
                    <textcolor>white</textcolor>
                    <shadowcolor>80000000</shadowcolor>
                    <align>center</align>
                    <aligny>center</aligny>
                    <info>ListItem.Label</info>
                </control>
            </itemlayout>
            <focusedlayout width="70" height="70">
                <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>50</width>
                    <height>50</height>        
                    <texture>search-input-small-focus.png</texture>
                    <aspectratio>stretch</aspectratio>
                </control>    
                <control type="label">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>50</width>
                    <height>50</height>   
                    <font>font8</font>
                    <textcolor>ff0080ff</textcolor>
                    <shadowcolor>80000000</shadowcolor>
                    <align>center</align>
                    <aligny>center</aligny>
                    <info>ListItem.Label</info>
                </control>
            </focusedlayout>
        </control>
        <control type="label" id="1211">
            <posx>170</posx>
            <posy>110</posy>
            <width>400</width>
            <height>40</height>
            <font>font14</font>
            <textcolor>white</textcolor>
            <shadowcolor>80000000</shadowcolor>
            <align>center</align>
            <aligny>center</aligny>
        </control>
        <control type="label" id="1212">
            <posx>570</posx>
            <posy>110</posy>
            <width>400</width>
            <height>40</height>
            <font>font14</font>
            <textcolor>grey</textcolor>
            <shadowcolor>80000000</shadowcolor>
            <align>center</align>
            <aligny>center</aligny>
        </control>
        <control type="panel" id="1220">
            <description>guess-search</description>
            <posx>720</posx>
            <posy>165</posy>
            <width>400</width>
            <height>470</height>
            <viewtype>list</viewtype>
            <onleft>1210</onleft>
            <onup>-</onup>
            <ondown>-</ondown>
            <itemlayout width="400" height="70">
                <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>400</width>
                    <height>50</height>        
                    <texture>search-choices.png</texture>
                    <aspectratio>stretch</aspectratio>
                </control>    
                <control type="label">
                    <posx>10</posx>
                    <posy>0</posy>
                    <width>390</width>
                    <height>50</height>   
                    <font>font8</font>
                    <textcolor>white</textcolor>
                    <shadowcolor>80000000</shadowcolor>
                    <align>left</align>
                    <aligny>center</aligny>
                    <info>ListItem.Label</info>
                </control>
            </itemlayout>
            <focusedlayout width="70" height="70">
                <control type="image">
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>400</width>
                    <height>50</height>        
                    <texture>search-choices-focus.png</texture>
                    <aspectratio>stretch</aspectratio>
                </control>    
                <control type="label">
                    <posx>10</posx>
                    <posy>0</posy>
                    <width>390</width>
                    <height>50</height>   
                    <font>font8</font>
                    <textcolor>ff0080ff</textcolor>
                    <shadowcolor>80000000</shadowcolor>
                    <align>left</align>
                    <aligny>center</aligny>
                    <info>ListItem.Label</info>
                </control>
            </focusedlayout>
        </control>
    </controls>
</window>

